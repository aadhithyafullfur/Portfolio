{
  "buildCommand": "CI=false npm run build",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
